<h1>Accesstokens#index</h1>
<%= link_to 'tech-conn', controller: 'accesstokens' %>

<table>
	<tr>
		<th>Firstname </th>
		<th>Lastname </th> 
		<th>Token</th>
		<th colspan="3"></th>
	</tr>

	<% @accesstokens.each do |t| %>
		<tr>
			<td><%= t.firstname %></td>
			<td><%= t.lastname %></td>
			<td><%= t.token %></td>
		</tr>
	<% end %>
</table>